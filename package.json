{
  "name": "no-gray",
  "displayName": "no-gray",
  "version": "0.0.0",
  "description": "I don't want to care about the life or death of people who have nothing to do with me.",
  "author": "yuanqinglong",
  "scripts": {
    "preinstall": "husky install",
    "commit": "git add . && git-cz",
    "dev": "plasmo dev --no-source-maps",
    "build": "plasmo build --zip"
  },
  "dependencies": {
    "@plasmohq/storage": "^1.7.2",
    "antd": "5.8.2",
    "plasmo": "0.82.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "plasmo": "0.82.0",
    "@plasmohq/storage": "^1.7.2",
    "@plasmohq/prettier-plugin-sort-imports": "4.0.1",
    "@types/chrome": "0.0.243",
    "@types/node": "20.4.8",
    "@types/react-dom": "18.2.7",
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@types/react": "^18.2.18",
    "@umijs/fabric": "^4.0.1",
    "commitizen": "^4.3.0",
    "cz-git": "^1.7.0",
    "husky": "^8.0.3"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  }
}
